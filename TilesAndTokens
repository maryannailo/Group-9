public enum TilesAndTokens {
    FOREST(HabitatAndWildlife.FOREST, TilesAndTokensColour.DARKGREEN),
    WETLAND(HabitatAndWildlife.WETLAND, TilesAndTokensColour.LIGHTGREEN),
    RIVER(HabitatAndWildlife.RIVER, TilesAndTokensColour.BLUE),
    MOUNTAIN(HabitatAndWildlife.MOUNTAIN, TilesAndTokensColour.GREY),
    PRAIRIE(HabitatAndWildlife.PRAIRIE, TilesAndTokensColour.YELLOW),
    FORESTKEY(HabitatAndWildlife.FORESTKEY, TilesAndTokensColour.DARKGREEN),
    WETLANDKEY(HabitatAndWildlife.WETLANDKEY, TilesAndTokensColour.LIGHTGREEN),
    RIVERKEY(HabitatAndWildlife.RIVERKEY, TilesAndTokensColour.BLUE),
    MOUNTAINKEY(HabitatAndWildlife.MOUNTAINKEY, TilesAndTokensColour.GREY),
    PRAIRIEKEY(HabitatAndWildlife.PRAIRIEKEY, TilesAndTokensColour.YELLOW),
    KEY(HabitatAndWildlife.KEY, TilesAndTokensColour.PURPLE),
    HAWK(HabitatAndWildlife.HAWK, TilesAndTokensColour.BLUE),
    HAWKTOKEN(HabitatAndWildlife.HAWKTOKEN, TilesAndTokensColour.BLACK),
    BEAR(HabitatAndWildlife.BEAR, TilesAndTokensColour.BROWN),
    BEARTOKEN(HabitatAndWildlife.BEARTOKEN, TilesAndTokensColour.BLACK),
    ELK(HabitatAndWildlife.ELK, TilesAndTokensColour.BLACK),
    ELKTOKEN(HabitatAndWildlife.ELKTOKEN, TilesAndTokensColour.BLACK),
    SALMON(HabitatAndWildlife.SALMON, TilesAndTokensColour.RED),
    SALMONTOKEN(HabitatAndWildlife.SALMONTOKEN, TilesAndTokensColour.BLACK),
    FOX(HabitatAndWildlife.FOX, TilesAndTokensColour.ORANGE),
    FOXTOKEN(HabitatAndWildlife.FOXTOKEN, TilesAndTokensColour.BLACK),
    NATURE(HabitatAndWildlife.NATURE, TilesAndTokensColour.DARKGREEN);
    private HabitatAndWildlife symbol;
    private TilesAndTokensColour colour;
    
    TilesAndTokens (HabitatAndWildlife symbol, TilesAndTokensColour colour) {
        this.symbol = symbol;
        this.colour = colour;
    }


    public TilesAndTokensColour getColour () {
        return colour;
    }
    public String toString(){return String.valueOf(symbol);}

    public HabitatAndWildlife getSymbol() {
        return symbol;
    }
}
